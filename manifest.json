{
  "manifest_version": 2,

  "name": "Project Access",

  "version": "1.0",

  "content_scripts":
  [
    {
      "matches":
      [
        "https://www.youtube.com/", "http://www.youtube.com/"
      ],
      "html":[],
      "js": [],
      "css": []
    }
  ],

  "browser_action":
  {
    "default_icon": "icon.png",
    "default_popup": "access.html",
    "default_title": "Project Access"
  },

  "permissions":
  [
    "tab",
    "<all_urls>"
  ],

  "background":
  {
    "scripts": ["foo.js","foo1.js"],
    "persistent": false
  }
}
